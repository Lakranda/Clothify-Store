<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserDashboardFormController">
   <children>
      <Label alignment="CENTER" layoutY="1.0" prefHeight="56.0" prefWidth="600.0" style="-fx-background-color: silver;" text="User Dashboard">
         <font>
            <Font name="Arial Black" size="36.0" />
         </font>
      </Label>
      <Pane layoutX="258.0" layoutY="71.0" prefHeight="314.0" prefWidth="327.0" style="-fx-background-color: silver;">
         <children>
            <Button layoutX="18.0" layoutY="126.0" mnemonicParsing="false" onAction="#btnProductOnAction" prefHeight="64.0" prefWidth="290.0" text="Product">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="18.0" layoutY="31.0" mnemonicParsing="false" onAction="#btnOrderOnAction" prefHeight="64.0" prefWidth="290.0" text="Order">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="18.0" layoutY="220.0" mnemonicParsing="false" onAction="#btnReportsOnAction" prefHeight="64.0" prefWidth="290.0" text="Reports">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </Pane>
      <Pane layoutX="14.0" layoutY="71.0" prefHeight="314.0" prefWidth="235.0" style="-fx-background-color: silver;">
