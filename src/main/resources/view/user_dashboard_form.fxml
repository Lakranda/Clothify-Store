            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="18.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true" />
            <Label layoutX="30.0" layoutY="179.0" prefHeight="22.0" prefWidth="175.0" text="Label" textFill="WHITE" />
            <Label layoutX="30.0" layoutY="208.0" prefHeight="22.0" prefWidth="175.0" text="Label" textFill="WHITE" />
            <Label layoutX="30.0" layoutY="239.0" prefHeight="22.0" prefWidth="175.0" text="Label" textFill="WHITE" />
            <Button layoutX="80.0" layoutY="275.0" mnemonicParsing="false" onAction="#btnUserSettingOnAction" text="Settings" />
         </children></Pane>
   </children>
</AnchorPane>
